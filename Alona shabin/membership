colour= ['blue', 'black', 'red']
if 'pink' in colour:
    print("pink is in the list.")
if 'blue' not in colour:
    print("blue is not in the list.")


x = [1, 2, 3]
y = x  
z = [1, 2, 3]  

if x is y:
    print("x and y refer to the same object.")

if x is not z:
    print("x and z do not refer to the same object.")
